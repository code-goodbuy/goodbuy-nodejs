{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nodejs-webserver",
      "image": "goodbuy/nodejs-webserver:travis",
      "essential": false,
      "portMappings" : {
        "containerPort": 8080,
        "hostPort": 80
      },
      "memory": 500
    },
    {
      "environment": [
        {
          "name":"DBHost", "value": "${MONGODB_DEV_URI}"
        },
        {
          "name":"MONGODB_DEV_URI", "value": "${MONGODB_DEV_URI}"
        },
        {
          "name":"RABBITMQ_DEV_HOST", "value": "${RABBITMQ_DEV_HOST}"
        },
        {
          "name":"RABBITMQ_PORT", "value": "${RABBITMQ_PORT}"
        },
        {
          "name":"RABBITMQ_USER", "value": "${RABBITMQ_USER}"
        },
        {
          "name":"ACCESS_TOKEN_SECRET", "value": "${ACCESS_TOKEN_SECRET}"
        },
        {
          "name":"REFRESH_TOKEN_SECRET", "value": "${REFRESH_TOKEN_SECRET}"
        }
      ]
    }
  ]
}
